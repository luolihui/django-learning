#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/14256/vscodepreject/web/day11-webpack/node_modules/.store/less@3.9.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/14256/vscodepreject/web/day11-webpack/node_modules/.store/less@3.9.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/less@3.9.0/node_modules/less/bin/lessc" "$@"
else
  exec node  "$basedir/../.store/less@3.9.0/node_modules/less/bin/lessc" "$@"
fi
